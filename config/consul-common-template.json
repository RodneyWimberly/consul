{
  "addresses": {
    "http": "0.0.0.0",
    "https": "0.0.0.0"
  },
  "advertise_addr": "{{ GetInterfaceIP \"eth0\" }}",
  "bind_addr": "{{ GetInterfaceIP \"eth0\" }}",
  "ca_file": "${CONSUL_CERT_DIR}/consul-agent-ca.pem",
  "ca_path": "${CONSUL_CERT_DIR}",
  "check_update_interval": "0s",
  "client_addr": "0.0.0.0",
  "connect": {
    "enabled": true
  },
  "data_dir": "${CONSUL_DATA_DIR}",
  "datacenter": "${CONSUL_DATACENTER}",
  "disable_host_node_id": true,
  "disable_remote_exec": true,
  "disable_update_check": true,
  "dns_config": {
    "allow_stale": false
  },
  "encrypt": "jgtdlbpgSBj8YoJGkuf4jw+Aa7it9dQeWm4yyBqHIVU=",
  "encrypt_verify_incoming" : true,
  "encrypt_verify_outgoing" : true,
  "http_config": {
    "response_headers": {
      "Access-Control-Allow-Origin": "*"
    }
  },
  "ports": {
    "http": 8500,
    "https": 8501
  },
  "node_meta": {
    "instance_type": "Docker Swarm",
    "container_id": "${CONTAINER_ID}",
    "container_name": "${CONTAINER_NAME}",
    "container_ip": "${CONTAINER_IP}",
    "manager_nodes_count": "${NUM_OF_MGR_NODES}",
    "manager_node": "${NODE_IS_MANAGER}",
    "node_name": "${NODE_NAME}",
    "node_ip": "${NODE_IP}"
  },
  "node_name": "${NODE_NAME}",
  "leave_on_terminate" : true,
  "log_level": "debug",
  "server": ${NODE_IS_MANAGER},
  "skip_leave_on_interrupt" : false,
  "telemetry": {
    "disable_compat_1.9": true
  },
  "ui_config": {
    "enabled": true
  },
  "verify_incoming": true,
  "verify_incoming_https" : true,
  "verify_incoming_rpc" : true,
  "verify_outgoing": true,
  "verify_server_hostname": false
}
